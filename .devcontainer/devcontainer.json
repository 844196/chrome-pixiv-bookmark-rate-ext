{
  "name": "chrome-pixiv-bookmark-rate-ext",
  "features": {
    "common": true,
  },
  "dockerComposeFile": [
    "../docker-compose.yml",
  ],
  "service": "workspace",
  "remoteUser": "node",
  "workspaceFolder": "/workspaces/chrome-pixiv-bookmark-rate-ext",
  "postCreateCommand": "npm install",
  "extensions": [
    "dbaeumer.vscode-eslint",
  ],
  "settings": {
    "typescript.tsdk": "node_modules/typescript/lib",
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true,
    },
    "files.associations": {
      "**/.github/renovate.json5": "jsonc",
    },
    "eslint.runtime": "/usr/local/bin/node",
  },
}
